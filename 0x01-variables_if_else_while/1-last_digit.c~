#include <stdlib.h>
#include <time.h>
/* more headers goes there */

/* betty style doc for function main goes there */
int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	int last_digit = abs(n % 10);
	printf("the last digit is %d\n", last_digit);
	printf("the digit n is %d\n", n);
	return (0);
}
